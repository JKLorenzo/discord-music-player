{
  "name": "discord-music-player",
  "version": "9.1.1",
  "description": "Complete framework to facilitate music commands using discord.js v14 and v13.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "test": "jest test/test.js",
    "docs:test": "docgen --jsdoc jsdoc.json --source src/*.ts src/**/*.ts --custom docs/custom.yml",
    "docs": "docgen --jsdoc jsdoc.json --source src/*.ts src/**/*.ts --custom docs/custom.yml --output docs/docs.json",
    "prepare": "npm run build"
  },
  "keywords": [
    "music",
    "player",
    "bot",
    "framework",
    "discord",
    "youtube",
    "spotify",
    "v14",
    "v13"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SushiBtw/discord-music-player.git"
  },
  "bugs": {
    "url": "https://github.com/SushiBtw/discord-music-player/issues"
  },
  "homepage": "https://discord-music-player.js.org/",
  "author": "SushiBtw",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "apple-music-metadata": "^2.0.1",
    "axios": "^1.13.2",
    "axios-retry": "^4.5.0",
    "discord-ytdl-core": "^5.0.4",
    "domhandler": "^5.0.3",
    "ffmpeg-static": "^5.3.0",
    "htmlparser2": "^10.0.0",
    "isomorphic-unfetch": "^4.0.2",
    "libsodium-wrappers": "^0.7.16",
    "spotify-url-info": "^3.2.18",
    "youtubei": "^1.8.3",
    "ytdl-core": "^4.11.5",
    "ytsr": "^3.8.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "assert-diff": "^3.0.4",
    "discord.js": "^14.25.1",
    "discord.js-docgen": "github:discordjs/docgen#ts-patch",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "jsdoc-babel": "^0.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  }
}
