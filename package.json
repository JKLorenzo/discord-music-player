{
  "name": "discord-music-player",
  "version": "9.1.1",
  "description": "Complete framework to facilitate music commands using discord.js v14 and v13.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "test": "jest test/test.js",
    "docs:test": "docgen --jsdoc jsdoc.json --source src/*.ts src/**/*.ts --custom docs/custom.yml",
    "docs": "docgen --jsdoc jsdoc.json --source src/*.ts src/**/*.ts --custom docs/custom.yml --output docs/docs.json",
    "prepare": "npm run build"
  },
  "keywords": [
    "music",
    "player",
    "bot",
    "framework",
    "discord",
    "youtube",
    "spotify",
    "v14",
    "v13"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SushiBtw/discord-music-player.git"
  },
  "bugs": {
    "url": "https://github.com/SushiBtw/discord-music-player/issues"
  },
  "homepage": "https://discord-music-player.js.org/",
  "author": "SushiBtw",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.18.0",
    "apple-music-metadata": "^2.0.1",
    "axios": "^1.7.9",
    "axios-retry": "^4.5.0",
    "discord-ytdl-core": "^5.0.4",
    "discord.js": "^14.18.0",
    "domhandler": "^5.0.3",
    "ffmpeg-static": "^5.2.0",
    "htmlparser2": "^10.0.0",
    "isomorphic-unfetch": "^4.0.2",
    "libsodium-wrappers": "^0.7.15",
    "spotify-url-info": "^3.2.18",
    "youtubei": "^1.6.7",
    "ytdl-core": "^4.11.5",
    "ytsr": "^3.8.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "assert-diff": "^3.0.4",
    "discord.js-docgen": "github:discordjs/docgen#ts-patch",
    "eslint": "^9.21.0",
    "jest": "^29.7.0",
    "jsdoc-babel": "^0.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  }
}
